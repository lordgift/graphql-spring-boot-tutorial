type Query {
    pets: [Pet]
    homeless: [Homeless]

}

type Mutation {
    createUser(user: User):Boolean
}

input User {
    name: String
    position: String
}


type Pet {
    id: Int!
    type: Animal!
    name: String
    age: Int
}

type Homeless {
    id: Int!
    type: Animal!
    location: String
}

enum Animal {
    DOG
    CAT
    BADGER
    MAMMOTH
}